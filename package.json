{
  "name": "jordan-lawbot-backend",
  "version": "1.0.0",
  "description": "AI-powered legal consultation platform for Jordanian lawyers",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install:client": "cd client && npm install",
    "install:all": "npm install && cd client && npm install",
    "heroku-postbuild": "npm run install:client && npm run build:client",
    "vercel-build": "npm run install:client && npm run build:client",
    "seed": "node scripts/seedDatabase.js",
    "migrate": "node scripts/migrate.js"
  },
  "keywords": [
    "legal",
    "ai",
    "chatbot",
    "jordan",
    "law",
    "consultation",
    "saas",
    "openai",
    "mongodb",
    "express"
  ],
  "author": "Jordan LawBot Team",
  "license": "MIT",
  "dependencies": {
    "archiver": "^5.3.1",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "dayjs": "^1.11.13",
    "mongoose": "^8.8.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "openai": "^4.73.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "redis": "^4.7.0",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "stripe": "^17.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "validator": "^13.12.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/jest": "^29.5.5",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.2",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/jordan-lawbot.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/jordan-lawbot/issues"
  },
  "homepage": "https://github.com/your-username/jordan-lawbot#readme",
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!client/**",
      "!coverage/**",
      "!tests/**"
    ]
  }
}
